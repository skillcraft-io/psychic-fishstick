{
    "name": "skillcraft-io/psychic-fishstick",
    "description": "A Laravel package for creating structured and reusable pipelines with support for custom and generic workflows, making sequential data processing efficient and flexible.",
    "type": "library",
    "keywords": [
        "laravel",
        "pipeline",
        "data processing",
        "workflow",
        "middleware"
    ],
    "homepage": "https://github.com/skillcraft-io/psychic-fishstick",
    "license": "MIT",
    "authors": [
        {
            "name": "William Troiano",
            "email": "william.troiano@pm.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3||^8.4",
        "illuminate/pipeline": "^10.0||^11.0",
        "illuminate/events": "^10.0||^11.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "larastan/larastan": "^2.9",
        "orchestra/testbench": "^9.0.0||^8.22.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-arch": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-phpunit": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Skillcraft\\PsychicFishstick\\": "src/",
            "Skillcraft\\PsychicFishstick\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Skillcraft\\PsychicFishstick\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Skillcraft\\PsychicFishstick\\Providers\\PsychicFishstickServiceProvider"
            ],
            "aliases": {
                "PsychicFishstick": "Skillcraft\\PsychicFishstick\\Facades\\PsychicFishstick"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
